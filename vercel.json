{
  "version": 2,
  "builds": [
    {
      "src": "api-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/analyze",
      "dest": "/api-server.js"
    },
    {
      "src": "/health",
      "dest": "/api-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
} 